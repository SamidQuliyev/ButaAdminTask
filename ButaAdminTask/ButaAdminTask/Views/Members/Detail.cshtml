
@model Members
@{ ViewData["Title"] = "Detail"; }

<div class="row">

    <div class="col-lg-12 grid-margin stretch-card">
        <div class="card">
            <div class="card-body">

             
                <h4> İştirakçı : @Model.FullName</h4>
                <div> Yaşı : @Model.Age</div>
                <div> Cinsi : @Model.Gender</div>
                <div> Təhsili : @Model.Education</div>
                <div> Universiteti : @Model.University</div>
                <div> İxtisası: @Model.Profession</div>
                <div> Kursu: @Model.Course</div>




                <a class="btn btn-secondary  btn-sm my-3" asp-action="Index">Geriyə</a>
            </div>
        </div>
    </div>
</div>

